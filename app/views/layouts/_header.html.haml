%head
  %title idac :: Ideas at CWRU
  = stylesheet_link_tag 'main'
  = stylesheet_link_tag 'print', :media => "print"
  = stylesheet_link_tag 'cwruheader'
  = stylesheet_link_tag 'lightwindow/lightwindow.css'
  = javascript_include_tag 'nav'
  = javascript_include_tag :defaults
  = javascript_include_tag 'lightwindow.js'
  :javascript
    document.observe("dom:loaded", function() {
      new Effect.BlindDown('topIdea', {duration: .75});
    });
    
    function kill(el){
      new Effect.BlindUp(el);
    }
